{
    "name": "canvass/canvass-paint-blade",
    "description": "A form rendering library using Laravel's Blade",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Kelly",
            "email": "puritandesigns@gmail.com"
        }
    ],
    "require": {
        "canvass/canvass-paint": "^1.02"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.3",
        "illuminate/support": "^6.2",
        "laravel/framework": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "CanvassPaint\\Blade\\": "src/CanvassPaint/Blade/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CanvassPaint\\Blade\\CanvassPaintServiceProvider"
            ]
        }
    },
    "config": {
        "platform-check": false,
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
